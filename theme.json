{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [
						"#F1F2F2",
						"#F1F2F2"
					],
					"name": "Background and Foreground",
					"slug": "background-and-foreground"
				},
				{
					"colors": [
						"#C14420",
						"#F1F2F2"
					],
					"name": "Foreground and background",
					"slug": "foreground-and-background"
				}
			],
			"palette": [
				{
					"color": "#1d3557",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#f2f3f4",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#414751",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#065cb4",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#db504a",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "10px",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "20px",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "30px",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "clamp(30px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(30px, 7vw, 70px)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(50px, 7vw, 90px)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Cormorant",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/cormorant-normal-400.woff2"
						},
						{
							"fontFamily": "Cormorant",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/cormorant-normal-700.woff2"
						},
						{
							"fontFamily": "Cormorant",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": "file:./assets/fonts/cormorant-italic-400.woff2"
						},
						{
							"fontFamily": "Cormorant",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": "file:./assets/fonts/cormorant-italic-700.woff2"
						}
					],
					"fontFamily": "Cormorant, serif",
					"name": "Cormorant",
					"slug": "cormorant"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/inter-normal-400.woff2"
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/inter-normal-700.woff2"
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|foreground",
					"text": "var:preset|color|background"
				}
			},
			"core/code": {
				"border": {
					"color": "#CCCCCC",
					"radius": "0px",
					"style": "solid",
					"width": "2px"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|gigantic"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"color": "var:preset|color|primary"
				}
			}
		},
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|black"
		},
		"spacing": {
			"blockGap": "1.2rem",
			"padding": {
				"left": "var:preset|spacing|50",
				"right": "var:preset|spacing|50"
			}
		},
		"css": "/* Format nav button on mobile */\n.wp-block-navigation__responsive-container-open {\n  border: 0.125rem solid rgba(1,1,1,0.25);\n  border-radius: 10%;\n  padding: 0.25rem 0.4rem;\n}\n\n/* Hide some nav elements on mobile */\n@media(max-width: 599px){\n  header .wp-block-spacer {\n    display: none;\n  }\n  .mobile-flex > .wp-block-site-title {\n    font-size: var(--wp--preset--font-size--huge);\n  }\n  .mobile-flex {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    align-items: center;\n  }\n  .mobile-hide {\n    display: none;\n  }\n}",
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|secondary"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|huge"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|extra-large"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|large"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|normal"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|small"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|tiny"
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|foreground"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|foreground"
					}
				},
				"color": {
					"text": "var:preset|color|secondary"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|inter"
		}
	},
	"templateParts": [
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3
}